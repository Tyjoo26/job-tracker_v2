All Jobs for <strong><%= @company.name %></strong>

<% @jobs.each do |job| %>
  <ul><%= job.title %> at <%= job.company.name %></ul>
  <ul><%= link_to "Edit", edit_company_job_path(@company, job) %> </ul>
  <ul><%= link_to "Delete", company_job_path(@company, job), :data => {:confirm => "Confirm Delete?", :method => "delete"}%></ul>
<% end %>
