<h1>Category Details</h1><br>

<%= @category.title %><br>
<%= link_to "Edit", edit_category_path(@category) %><br>
<%= link_to "Delete", category_path(@category), :data => {:confirm => "Confirm Delete?", :method => "delete"} %><br>
<div class="">
  <% @category.jobs.each do |job| %>
  <%= job.title %>
  <%= job.description %>
  <%= job.level_of_interest %>
  <%= job.city %>
  <% end %>
</div>

<%= link_to "Go Back to Categories Index", categories_path %>
